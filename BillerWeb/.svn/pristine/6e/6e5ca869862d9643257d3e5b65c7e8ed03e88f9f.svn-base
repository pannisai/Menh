<?page title="Dialog" contentType="text/html;charset=UTF-8"?>
<zk>
	<zscript>
	public void getDialogTitle(Window w, Include i) {
		w.setTitle(((Label) i.getFellow("title")).getValue());
	}
</zscript>
	<window id="bw1440Dialog" onCreate="getDialogTitle(self,bw1440DialogInclude)" border="normal" width="95%"
		height="370px" position="center,center" closable="true"
		action="show: slideDown;hide: slideUp">
		<include id="bw1440DialogInclude" dialog="true" />
	</window>
</zk>