<?page title="${labels.BW2210_EDIT_TITLE}" contentType="text/html;charset=UTF-8"?>
<zk>
	<zscript>
	public void getDialogTitle(Window w, Include i) {
		w.setTitle(((Label) i.getFellow("title")).getValue());
	}
</zscript>
<window id="bw2210Dialog" onCreate="getDialogTitle(self,bw2210DialogInclude)" border="normal" width="95%" height="400px"
    position="center,center" closable="true"
    action="show: slideDown;hide: slideUp" >
		<include id="bw2210DialogInclude"  dialog="true"/>
</window>
</zk>