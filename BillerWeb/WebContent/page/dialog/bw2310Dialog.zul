<?page title="${labels.BW2310_EDIT_TITLE}" contentType="text/html;charset=UTF-8"?>
<zk>
	<zscript>
	public void getDialogTitle(Window w, Include i) {
		w.setTitle(((Label) i.getFellow("title")).getValue());
	}
</zscript>
<window id="bw2310Dialog" onCreate="getDialogTitle(self,bw2310DialogInclude)" border="normal" width="95%" height="350px"
    position="center,center" closable="true"
    action="show: slideDown;hide: slideUp" >
		<include id="bw2310DialogInclude"  dialog="true"/>
</window>
</zk>