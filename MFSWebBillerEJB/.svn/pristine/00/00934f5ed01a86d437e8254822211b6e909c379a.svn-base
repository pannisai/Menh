package test;

import java.io.Serializable;
import javax.persistence.*;
import java.util.Date;


/**
 * The persistent class for the BANK_CHANNEL database table.
 * 
 */
@Entity
@Table(name="BANK_CHANNEL")
public class BankChannel implements Serializable {
	private static final long serialVersionUID = 1L;

	@Id
	@GeneratedValue(strategy=GenerationType.IDENTITY)
	@Column(name="BANK_CHNL_CODE")
	private String bankChnlCode;

	@Column(name="ACT_FLAG")
	private String actFlag;

	@Column(name="BANK_CHNL_FULL_NAME_EN")
	private String bankChnlFullNameEn;

	@Column(name="BANK_CHNL_FULL_NAME_TH")
	private String bankChnlFullNameTh;

	@Column(name="BANK_CHNL_NAME")
	private String bankChnlName;

	@Column(name="CRTD_BY")
	private String crtdBy;

	@Temporal(TemporalType.DATE)
	@Column(name="CRTD_DTTM")
	private Date crtdDttm;

	@Column(name="LAST_CHNG_BY")
	private String lastChngBy;

	@Temporal(TemporalType.DATE)
	@Column(name="LAST_CHNG_DTTM")
	private Date lastChngDttm;

	public BankChannel() {
	}

	public String getBankChnlCode() {
		return this.bankChnlCode;
	}

	public void setBankChnlCode(String bankChnlCode) {
		this.bankChnlCode = bankChnlCode;
	}

	public String getActFlag() {
		return this.actFlag;
	}

	public void setActFlag(String actFlag) {
		this.actFlag = actFlag;
	}

	public String getBankChnlFullNameEn() {
		return this.bankChnlFullNameEn;
	}

	public void setBankChnlFullNameEn(String bankChnlFullNameEn) {
		this.bankChnlFullNameEn = bankChnlFullNameEn;
	}

	public String getBankChnlFullNameTh() {
		return this.bankChnlFullNameTh;
	}

	public void setBankChnlFullNameTh(String bankChnlFullNameTh) {
		this.bankChnlFullNameTh = bankChnlFullNameTh;
	}

	public String getBankChnlName() {
		return this.bankChnlName;
	}

	public void setBankChnlName(String bankChnlName) {
		this.bankChnlName = bankChnlName;
	}

	public String getCrtdBy() {
		return this.crtdBy;
	}

	public void setCrtdBy(String crtdBy) {
		this.crtdBy = crtdBy;
	}

	public Date getCrtdDttm() {
		return this.crtdDttm;
	}

	public void setCrtdDttm(Date crtdDttm) {
		this.crtdDttm = crtdDttm;
	}

	public String getLastChngBy() {
		return this.lastChngBy;
	}

	public void setLastChngBy(String lastChngBy) {
		this.lastChngBy = lastChngBy;
	}

	public Date getLastChngDttm() {
		return this.lastChngDttm;
	}

	public void setLastChngDttm(Date lastChngDttm) {
		this.lastChngDttm = lastChngDttm;
	}

}