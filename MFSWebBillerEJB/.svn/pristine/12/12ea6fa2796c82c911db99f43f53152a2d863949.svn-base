package test;

import java.util.List;

import javax.persistence.EntityManager;
import javax.persistence.EntityManagerFactory;
import javax.persistence.Persistence;
import javax.persistence.Query;

public class TestMain {
	private static final String PERSISTENCE_UNIT_NAME = "mfs.db.MFSDAO";
	  private  EntityManagerFactory factory;
	  
	
	public void test() {
	System.setProperty("javax.xml.parsers.DocumentBuilderFactory","org.apache.xerces.jaxp.DocumentBuilderFactoryImpl");
	System.setProperty("javax.xml.parsers.SAXParserFactory","org.apache.xerces.jaxp.SAXParserFactoryImpl");
		factory = Persistence.createEntityManagerFactory(PERSISTENCE_UNIT_NAME);
		EntityManager em = factory.createEntityManager();
		  System.out.println(em);
		Query q = em.createQuery("select bc from BankChannel bc");
		List<BankChannel> bankChannels = q.getResultList();
		 for (BankChannel bankChannel : bankChannels) {
		 System.out.println(bankChannel.getBankChnlCode());
		 }
		System.out.println("Size: " + bankChannels.size());

	}


	public static void main(String[] args) {
		TestMain tm = new TestMain();
		tm.test();
	}
}
